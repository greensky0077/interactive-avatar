{
  "version": 2,
  "builds": [
    { "src": "react-avatar-app/package.json", "use": "@vercel/static-build", "config": { "distDir": "react-avatar-app/dist" } },
    { "src": "api/index.mjs", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(persona|openai|pdf)(/.*)?", "dest": "/api/index.mjs" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}


